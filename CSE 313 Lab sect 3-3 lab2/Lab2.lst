   161                                  ;*_Dec2Bin:
   162                                  ;* This function converts an integer by dividing the number by 2. The quotent
   163                                  ;* will be stored in the rax, while the remander is stored in the rdx. The
   164                                  ;* value in the rdx will have 48 added to is value to get the ascii char back.
   165                                  ;* This value will then be stored in the valueBin to be printed in the last 
   166                                  ;* loop
   167                                  ;******************************************************************************
   168                                  _Dec2Bin:
   169 0000015D 48B9-                       mov rcx, valueBin                   ;set valueBin to rcx
   169 0000015F [0600000000000000] 
   170 00000167 BB0A000000                  mov rbx, 10                         ;mov newline char to ebx
   171 0000016C 488919                      mov [rcx], rbx                      ;add newline to rcx
   172 0000016F 48FFC1                      inc rcx                             
   173 00000172 48890C25[66000000]          mov [valueBinPos], rcx              ;move that value into ValueBinPos
   174                                  
   175                                  _loop1:
   176 0000017A BA00000000                  mov rdx, 0
   177 0000017F BB02000000                  mov rbx, 2                          
   178 00000184 48F7F3                      div rbx                             ;divideds rax by rbx
   179 00000187 50                          push rax                            ;mov rax to stack
   180 00000188 4883C230                    add rdx, 48                         ;add 48 to get asscii char
   181                                  
   182 0000018C 488B0C25[66000000]          mov rcx, [valueBinPos]              ;move valueBinPos to rcx
   183 00000194 8811                        mov [rcx], dl                       ;store results from the dl
   184 00000196 48FFC1                      inc rcx                             
   185 00000199 48890C25[66000000]          mov [valueBinPos], rcx              ;get next char
   186                                  
   187 000001A1 58                          pop rax                             ;retrieve value from stack
   188 000001A2 4883F800                    cmp rax, 0                          ;check if rax it a zero (end of conversion)
   189 000001A6 75D2                        jne _loop1                          ;loop until rax is zero
   190                                  _loop2:
   191 000001A8 488B0C25[66000000]          mov rcx, [valueBinPos]              ;get the last value from valueBinPos
   192 000001B0 B801000000                  mov rax, 1                      
   193 000001B5 BF01000000                  mov rdi, 1
   194 000001BA 4889CE                      mov rsi, rcx
   195 000001BD BA01000000                  mov rdx, 1
   196 000001C2 0F05                        syscall                             ;print char to console
   197                                  
   198 000001C4 488B0C25[66000000]          mov rcx, [valueBinPos]              
   199 000001CC 48FFC9                      dec rcx
   200 000001CF 48890C25[66000000]          mov [valueBinPos], rcx              ;get next char
   201                                  
   202 000001D7 4881F9[06000000]            cmp rcx, valueBin                   ;Once valueBin and valueBinPos are equal we are done
   203 000001DE 7DC8                        jge _loop2                          ;loop if not
   204 000001E0 C3                          ret
