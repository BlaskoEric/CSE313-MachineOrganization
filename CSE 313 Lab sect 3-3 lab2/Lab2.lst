   107                                  ;*_atoi:
   108                                  ;* This function converts a string of ascii char to integers. 
   109                                  ;* By having an empty variable after value, we can access the
   110                                  ;* string in value from right to left. From there we can substract
   111                                  ;* 0x30 from each character to get the integer value. The value
   112                                  ;* in shift contains value of one at start and is multiplied by
   113                                  ;* 10 each loop. That value to multiplied by the interger from the
   114                                  ;* value string and added to the buffer. Buffer will hold the
   115                                  ;* final integer value of the string
   116                                  ;*****************************************************************
   117                                  _atoi:
   118 000000E5 4831C0                      xor rax, rax                        ;clear registers
   119 000000E8 4D31C0                      xor r8, r8
   120                                  _Set:    
   121 000000EB 480FB60B                    movzx rcx,byte [rbx]                ;mov a char to rcx
   122 000000EF 4883F900                    cmp rcx, 0                          ;compare it to 0 "null terminated'
   123 000000F3 7462                        je _dec                             ;jump to loop to decrement
   124 000000F5 4883F90A                    cmp rcx, 10                         ;compare it to 10 "new line"
   125 000000F9 745C                        je _dec                             ;jump to loop to decrement
   126 000000FB 4883F90D                    cmp rcx, 13                         ;compare it to 13 "cariage return'
   127 000000FF 7456                        je _dec                             ;jump to loop to decrement
   128 00000101 48FFCB                      dec rbx                             ;once pasted previous coditions decrement once more
   129                                  _top:
   130                                  
   131 00000104 4883F930                    cmp rcx, '0'                        ;check if the characters value is less than 0x30
   132 00000108 7252                        jb _done                            ;if it is we are done
   133 0000010A 4883F939                    cmp rcx, '9'                        ;check if the characters value is more than 0x39
   134 0000010E 774C                        ja _done                            ;if it is we are done
   135 00000110 4883E930                    sub rcx, '0'                        ;else subtract 0x30
   136                                      
   137 00000114 4889CA                      mov rdx, rcx                        ;add that value to rdx
   138 00000117 4889C8                      mov rax, rcx                        ;and also rax
   139 0000011A 4C8B0425[04000000]          mov r8, [shift]                     ;move value of shift to r8 (starts at 1)
   140 00000122 49F7E0                      mul r8                              ;multiply r8 with rax
   141                                  
   142 00000125 48030425[54000000]          add rax, [buffer]                   ;add contents of buffer to rax
   143 0000012D 48890425[54000000]          mov [buffer], rax                   ;and move value back to buffer variable
   144                                      
   145 00000135 488B0425[04000000]          mov rax, [shift]                    ;mov shift value to rax
   146 0000013D 41B80A000000                mov r8, 10                          ;move r8 value of 10
   147 00000143 49F7E0                      mul r8                              ;multiply r8 and rax 
   148 00000146 48890425[04000000]          mov [shift], rax                    ;store the value back to shift
   149 0000014E 480FB60B                    movzx rcx, byte [rbx]               ;get the next character
   150 00000152 48FFCB                      dec rbx                             ;decrement rbx
   151                                  
   152 00000155 EBAD                        jmp _top                            ;loop until comparason results in _done
   153                                      
   154                                  _dec:
   155 00000157 48FFCB                      dec rbx                             ;decrement  and jumps back to _set
   156 0000015A EB8F                        jmp _Set
   157                                  _done:                                  ;once conversions done return to main
   158 0000015C C3                          ret
